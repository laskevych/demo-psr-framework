{
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3.0",
        "psr/http-message": "^1.0",
        "ramsey/uuid": "^3.8",
        "webmozart/assert": "^1.5",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "Code\\": "src/Code"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }

}
