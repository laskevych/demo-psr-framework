{
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3.0",
        "aura/router": "^3.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "ramsey/uuid": "^3.8",
        "webmozart/assert": "^1.5",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1",
        "zendframework/zend-stratigility": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App",
            "Core\\": "src/Core"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }

}
